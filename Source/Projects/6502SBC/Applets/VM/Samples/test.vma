.CONST
    Print.NewLine 0x14
    Print.String  0x11
    ArgGet        0x37

.DATA
    STR0 "r"

.MAIN
    ; Print newline
    SYSCALL Print.NewLine
    
    ; Get filename argument (arg 1)
    PUSHB 2
    POPA
    SYSCALL ArgGet
    ; ZP.STR now points to filename
    
    SYSCALL Print.String
    
    SYSCALL Print.NewLine
    
    HALT